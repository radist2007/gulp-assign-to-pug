{
  "name": "gulp-assign-to-jade",
  "version": "1.1.0",
  "description": "Assign file contents to the Jade template as a local variable",
  "repository": "shinnn/gulp-assign-to-jade",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "pretest": "eslint --config @shinnn/node index.js test/test.js",
    "test": "node --strong_mode test/test.js",
    "coverage": "node --strong_mode node_modules/.bin/istanbul cover --no-default-excludes -x=node_modules/** test/test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "files": [
    "index.js"
  ],
  "license": "MIT",
  "keywords": [
    "gulpplugin",
    "gulp",
    "jade",
    "compile",
    "assign",
    "variable",
    "template",
    "contents"
  ],
  "dependencies": {
    "fs-readfile-promise": "^2.0.1",
    "gulp-jade": "^1.1.0",
    "gulp-util": "^3.0.6",
    "replace-ext": "^0.0.1",
    "vinyl-bufferstream": "^1.0.1"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node": "^1.0.1",
    "eslint": "^1.4.1",
    "from2-string": "^1.1.0",
    "istanbul": "^0.3.19",
    "istanbul-coveralls": "^1.0.3",
    "tape": "^4.2.0",
    "vinyl": "^0.5.3"
  }
}
